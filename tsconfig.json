{
	"compilerOptions": {
		"baseUrl": ".",
		"rootDir": ".",
		"outDir": "dist",
		"sourceMap": false,
		"noEmit": true,
		"target": "es2022",
		"module": "es2022",
		"moduleResolution": "Bundler",
		"allowImportingTsExtensions": true,
		"lib": [
			"dom",
			"dom.iterable",
			"es2022"
		],
		"allowUnreachableCode": false,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUncheckedIndexedAccess": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"strict": true,
		"strictNullChecks": true,
		"types": [
			"vite/client"
		]
	},
	"include": [
		"./src/**/*.ts"
	]
}
